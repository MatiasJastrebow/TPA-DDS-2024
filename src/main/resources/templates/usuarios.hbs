{{#partial "estilo"}}
    <link rel="stylesheet" href="/css/usuarios.css">
{{/partial}}

{{#partial "cuerpo"}}
    <h2>Usuarios del Sistema</h2>
    <table class="usuarios-tabla">
        <thead>
        <tr>
            <th class="tabla-titulo" >Nombre</th>
            <th class="tabla-titulo" >Rol</th>
            <th class="tabla-titulo" >Fecha de Alta</th>
        </tr>
        </thead>
        <tbody>
        {{#each usuarios}}
            <tr>
                <td>{{this.getNombreUsuario}}</td>
                <td>{{this.getTipoRol}}</td>
                <td>{{this.getFechaAlta}}</td>
            </tr>
        {{/each}}
        </tbody>
    </table>
{{/partial}}

{{> templates/base }}
